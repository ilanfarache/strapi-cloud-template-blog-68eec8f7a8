{
  "kind": "collectionType",
  "collectionName": "catalogs",
  "info": {
    "singularName": "catalog",
    "pluralName": "catalogs",
    "displayName": "Catalog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "blocks"
    },
    "Price": {
      "type": "biginteger"
    },
    "pictures": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "catalogs"
    },
    "manufacturer": {
      "type": "string"
    },
    "year": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "new",
        "occasion"
      ]
    },
    "weight": {
      "type": "string"
    },
    "model": {
      "type": "string"
    },
    "revision": {
      "type": "string"
    },
    "address": {
      "type": "string"
    }
  }
}
